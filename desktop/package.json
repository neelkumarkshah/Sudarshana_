{
  "name": "Sudarshana - E-Rakshak",
  "version": "1.0.0",
  "description": "Web application and API's Security Testing Cross-platform Desktop Application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dev": "nodemon --exec electron ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neelkumarkshah/Sudarshana_.git"
  },
  "keywords": [
    "Web",
    "application",
    "API's",
    "Security",
    "Testing",
    "Automated",
    "Scanner"
  ],
  "author": "Neelkumar K. Shah",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/neelkumarkshah/Sudarshana_/issues"
  },
  "homepage": ".",
  "dependencies": {
    "@electron-forge/cli": "^7.8.1",
    "@fontsource/inter": "^5.2.5",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@hookform/resolvers": "^5.0.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "axios": "^1.9.0",
    "bcryptjs": "^3.0.2",
    "bootstrap": "^5.3.6",
    "chart.js": "^4.4.9",
    "chartjs-adapter-date-fns": "^3.0.0",
    "country-telephone-data": "^0.6.3",
    "dotenv": "^17.2.1",
    "electron": "^36.4.0",
    "electron-updater": "^6.6.2",
    "electron-window-state": "^5.0.3",
    "formik": "^2.4.6",
    "html2canvas": "^1.4.1",
    "intl-tel-input": "^25.3.1",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "mongodb": "^6.17.0",
    "nodemon": "^3.1.10"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "appId": "com.sudarshana.erakshak",
    "files": [
      "build/**/*",
      "main.js",
      "utils/**/*",
      "assets/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "mac": {
      "icon": "assets/sudarshana.icns",
      "target": "dmg"
    },
    "win": {
      "icon": "assets/sudarshana.ico",
      "target": "nsis"
    },
    "linux": {
      "icon": "assets/sudarshana.png",
      "target": "AppImage"
    }
  },
  "devDependencies": {
    "concurrently": "^9.1.2",
    "electron-builder": "^26.0.12",
    "electron-is-dev": "^3.0.1",
    "wait-on": "^8.0.3"
  }
}
